(struct struct-a a int)
(struct struct-b b (ptr int))
(struct struct-c c (array int 10))

(def func (a struct-a b struct-b c struct-c) (ptr int)
  (declare x int (+ 1 1))
  (declare y float (+ 1.1 1))
  (declare z float (+ x y (* x y)))
  (declare w (ptr int) (malloc (sizeof int)))
  (return w))

(def main() int
  (return 0))
